<ul>
	<li>
		<span class="total">total</span>
		<span class="value">{{= Dash.format(data.total) }}</span>
	</li>

	{{~ data.layers :m }}
		<li>
			<span class="title">{{= m.name }}</span>
			<span class="value">
				{{= Dash.format(m.total) }}
				<span class="key" style="background-color:{{= m.colour }}"></span>
			</span>
		</li>
	{{~}}
</ul>
